    "proxy-agent": "6.4.0",
    "socks-proxy-agent": "8.0.4",
    "superagent": "^10.1.0",
    "superagent-proxy": "3.0.0",
    "pac-proxy-agent": "7.0.2",
    "pac-resolver": "7.0.1",
    "degenerator": "5.0.1",
    "netmask": "2.0.2",
    "tough-cookie": "5.0.0",
    "yargs-parser": "21.1.1",
    "firebase-tools": "13.22.0",
    "router": "1.3.8",
    "superstatic": "9.0.3",
    "mem": "10.0.0",
    "braces": "3.0.3",
    "cookie": "1.0.0"
  },
  "devDependencies": {
    "coveralls": "3.1.1",
