    "eslint-plugin-node": "11.1.0",
    "jsdoc": "4.0.3",
    "jsdom": "25.0.1",
    "license-checker": "25.0.1",
    "mbTest": "file:./mbTest",
    "mocha": "10.7.3",
    "mocha-multi-reporters": "1.5.1",
    "nc": "^0.0.0",
    "nyc": "^17.1.0",
    "snyk": "1.1293.1"
  },
  "engines": {
    "node": ">=22"
  },
  "snyk": true
}
